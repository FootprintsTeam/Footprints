        <div class="row ">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="top-friend bg-green-light">
                    <div class="pull-left">
                        <p> <i class="fa icon-users"></i><span class="friends">Friends</span> <span>25 people</span></p>
                    </div>
                    <div class="pull-right">
                        <div class="form-ground ">
                            <form role="search" class="btn-search" id="search_mini_form">
                                <button class="button">
                                    <i class="fa fa-search "></i>
                                </button>
                                <input type="text" autocomplete="on" id="search" class="search-header text-input" name="q">
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <!--column 1-->

