        <!-- Content START -->
        @* Change id to generate form modal *@
        <div id="content-new">
            @{Html.RenderPartial("MainNavbar");}
            @*Render BODY*@
            @RenderBody()
        </div>
    </div>
    <!-- // Content END -->

