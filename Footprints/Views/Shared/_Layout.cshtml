    <script>
        var basePath = '',
       commonPath = '../assets/',
       rootPath = '../',
       DEV = false,
       componentsPath = '../assets/components/';

        var primaryColor = '#5577b4',
            dangerColor = '#b55151',
            successColor = '#609450',
            infoColor = '#4a8bc2',
            warningColor = '#ab7a4b',
            inverseColor = '#45484d';

        var themerPrimaryColor = primaryColor;
        var App = {};
        App.Config = {
            ajaxify_menu_selectors: ['#menu'],
            ajaxify_layout_app: false
        };
    </script>
    @Scripts.Render("~/script/core")
    @Scripts.Render("~/script/dependency")
    @Scripts.Render("~/script/plugins")
